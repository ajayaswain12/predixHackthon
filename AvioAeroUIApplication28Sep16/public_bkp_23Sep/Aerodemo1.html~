	 <script>

	 function initMap(flightId) {

  var map = new google.maps.Map(document.getElementById('map'), {

    zoom: 3,

    center: {lat: 7.498, lng: -44.869999999},

    mapTypeId: 'terrain'

  });

  var flightPlanCoordinates  = new Array(); 
  
  var mapUrl = "https://predix-aveoaeroflightservice9.run.aws-usw02-pr.ice.predix.io/getAllAvioAeroData3/"+flightId;
  $.getJSON(mapUrl, function(data) {

					

				for (i = 0; i < data.loc.length; i++){

					

					flightPlanCoordinates.push(new google.maps.LatLng(data.loc[i].latt, data.loc[i].long));



                }

  var flightPath = new google.maps.Polyline({

    path: flightPlanCoordinates,

    geodesic: true,

    strokeColor: '#FF0000',

    strokeOpacity: 1.0,

    strokeWeight: 2

  });

  flightPath.setMap(map);

  });

}

</script>





