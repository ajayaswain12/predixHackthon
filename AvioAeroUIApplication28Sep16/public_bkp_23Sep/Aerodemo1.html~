		 var urlForData = "https://predix-aveoaeroflightservice5.run.aws-usw02-pr.ice.predix.io/getAllAvioAeroData/"+$("#drpFlight").val();

		  var processed_json7 = new Array(); 

		 var processed_json8 = new Array();

		 var processed_json9 = new Array();

		  var processed_json10 = new Array(); 

		 var processed_json11 = new Array();

		 var processed_json12 = new Array();

		var processed_jsonC = new Array();  

 

                $.getJSON(urlForData, function(data) {

				

                    // Populate series

                    for (i = 0; i < data.flightParametersDtoList.length; i++){

                        processed_json7.push([data.flightParametersDtoList[i].e07]);

						processed_json8.push([data.flightParametersDtoList[i].e08]);

                    }

					

					for (i = 0; i < data.length; i++){

                        processed_jsonC.push([data.flightParametersDtoList[i].time]);

                    }

                 

                    // draw chart

                    $('#container1').highcharts({

                    chart: {

                        type: "spline"

                    },

                    title: {

                        text: "Flight Data"

                    },

                    xAxis: {

                       categories: processed_jsonC

                    },

                    yAxis: {

                        title: {

                            text: "Flight"

                        }

                    },

                    series: [{

	                    name: 'e07',

                        data: processed_json7

                    },

					{

	                    name: 'e08',

                        data: processed_json8

                    }]

                }); 

				

				//second graph

				

				for (i = 0; i < data.flightParametersDtoList.length; i++){

                        processed_json9.push([data.flightParametersDtoList[i].e60]);

						processed_json10.push([data.flightParametersDtoList[i].e61]);

                    }

					

					for (i = 0; i < data.length; i++){

                        processed_jsonC.push([data.flightParametersDtoList[i].time]);

                    }

                 

                    // draw chart

                    $('#container2').highcharts({

                    chart: {

                        type: "spline"

                    },

                    title: {

                        text: "Flight Data"

                    },

                    xAxis: {

                       categories: processed_jsonC

                    },

                    yAxis: {

                        title: {

                            text: "Flight"

                        }

                    },

                    series: [{

	                    name: 'e60',

                        data: processed_json9

                    },

					{

	                    name: 'e61',

                        data: processed_json10

                    }]

                }); 

	

				for (i = 0; i < data.flightParametersDtoList.length; i++){

                        processed_json11.push([data.flightParametersDtoList[i].l01]);

						processed_json12.push([data.flightParametersDtoList[i].m61]);

                    }

					

					for (i = 0; i < data.length; i++){

                        processed_jsonC.push([data.flightParametersDtoList[i].time]);

                    }

                 

